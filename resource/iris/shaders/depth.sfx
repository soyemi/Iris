#file depth.sfx
#include "basis.sfx"

#GLSL
precision mediump float;

in vec4 aPosition;
void vertex(){
    gl_Position = MATRIX_MVP * aPosition;
}

void fragment(){
}

#END

technique depth{
    vs: vertex;
    ps: fragment;
    pipeline:{
        queue: other;
    }
    property:{
        POSITION_0: aPosition;
    }
}